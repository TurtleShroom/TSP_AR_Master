  <!-- Roman -->
  <thingDef ParentName="RomeArmourHelmetMakeableBase">
    <defName>AR_Montefortino</defName>
    <label>Montefortino</label>
    <description>An early Roman helmet type copied from Celtic tribes.</description>
    <techLevel>Neolithic</techLevel>
    <graphicData>
      <texPath>Items/Apparel/MontefortinoOnMap</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
    </graphicData>
    <costStuffCount>40</costStuffCount>
    <stuffCategories>
      <li>Metallic</li>
      <li>Leathery</li>
      <li>Woody</li>
      <!-- Mod support -->
      <li MayRequire="Dubwise.Rimefeller">Plastic</li>
      <li MayRequire="Dubwise.Rimefeller">Composite</li>
      <li MayRequire="sarg.alphabiomes">AB_Exotic</li>
      <li MayRequire="sarg.alphabiomes">AB_ExoticWooden</li>
      <li MayRequire="sarg.alphabiomes">AB_ExoticMetals</li>
    </stuffCategories>
    <statBases>
      <WorkToMake>3500</WorkToMake>
      <MaxHitPoints>100</MaxHitPoints>
      <Mass>2</Mass>
      <StuffEffectMultiplierArmor>0.5</StuffEffectMultiplierArmor>
      <StuffEffectMultiplierInsulation_Cold>0.3</StuffEffectMultiplierInsulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>0.15</StuffEffectMultiplierInsulation_Heat>
      <EquipDelay>2.5</EquipDelay>
    </statBases>
    <equippedStatOffsets>
      <PsychicSensitivity>-0.10</PsychicSensitivity>
    </equippedStatOffsets>
    <apparel>
      <bodyPartGroups>
        <li>UpperHead</li>
        <li>Teeth</li>
      </bodyPartGroups>
      <wornGraphicPath>Items/Apparel/Hats/Montefortino</wornGraphicPath>
      <useWornGraphicMask>true</useWornGraphicMask>
      <layers>
        <li>Overhead</li>
      </layers>
      <tags>
        <li>RomeSoldier</li>
        <li>Barbarian</li>
        <li>ClassicalMilitary</li>
        <li>HellenicSoldier</li>
        <li>NeolithicMilitary</li>
        <li>Neolithic</li>
        <li>Tribal</li>
      </tags>
      <defaultOutfitTags>
        <li>Soldier</li>
      </defaultOutfitTags>
    </apparel>
    <recipeMaker>
      <skillRequirements>
        <Crafting>3</Crafting>
      </skillRequirements>
    </recipeMaker>
  </thingDef>
