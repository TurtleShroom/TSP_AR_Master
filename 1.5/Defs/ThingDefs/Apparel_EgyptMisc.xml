      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <costStuffCount>60</costStuffCount>
    <stuffCategories>
      <li>Fabric</li>
    </stuffCategories>
    <statBases>
		<MarketValue>9</MarketValue>
      <WorkToMake>2000</WorkToMake>
      <StuffEffectMultiplierArmor>0.1</StuffEffectMultiplierArmor>
      <StuffEffectMultiplierInsulation_Cold>0.16</StuffEffectMultiplierInsulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>1.25</StuffEffectMultiplierInsulation_Heat>
      <EquipDelay>1.5</EquipDelay>
      <Mass>0.4</Mass>
    </statBases>
    <apparel>
      <bodyPartGroups>
        <li>Legs</li>
        <li>Torso</li>
      </bodyPartGroups>
      <wornGraphicPath>Items/Apparel/SimpleDress/Simple</wornGraphicPath>
      <layers>
        <li>OnSkin</li>
      </layers>
      <tags>
        <li>EgyptianBasic</li>
		<li>EgyptianAdvanced</li>
		<li>EgyptianLight</li>
		<li>EgyptianMedium</li>
		<li>EgyptianHeavy</li>
		<li>Medieval</li>
		<li>NeolithicDecent</li>
		<li>OrientalLight</li>
		<li>OrientalMedium</li>
		<li>OrientalHeavy</li>
		<li>Tribal</li>
		<li>Neolithic</li>
		<li>EgyptDress</li>
      </tags>
      <gender>Female</gender>
	  <canBeGeneratedToSatisfyWarmth Inherit="False">true</canBeGeneratedToSatisfyWarmth>
      <defaultOutfitTags>
        <li>Worker</li>
		<li>Soldier</li>
        <li>Slave</li>
      </defaultOutfitTags>
    </apparel>
    <colorGenerator Class="ColorGenerator_Options">
      <options>
        <li>
          <weight>20</weight>
          <only>(1,1,1,1)</only>
        </li>
        <li>
          <weight>5</weight>
          <only>(0.7,0.63,0.2,1)</only>
        </li>
      </options>
    </colorGenerator>
    <recipeMaker>
      <researchPrerequisites>
		<li>AR_Project_Oriental_Culture</li>
	  </researchPrerequisites>
      <recipeUsers>
        <!-- This should inherit from the Base -->
        <li>CraftingSpot</li>
      </recipeUsers>
    </recipeMaker>
	  <tradeTags>
		<li>ClothingSimple</li>
		<li>Egypt</li>
		<li>ClothingBasic</li>
		<li>Clothing</li>
	  </tradeTags>
  </thingDef>
