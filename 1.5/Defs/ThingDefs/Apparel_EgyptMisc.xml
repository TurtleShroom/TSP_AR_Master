        <!-- This should be inherited -->
        <li>TableMachining</li>
      </recipeUsers>
      <skillRequirements>
        <Crafting>8</Crafting>
      </skillRequirements>
    </recipeMaker>
    <comps>
      <li Class="CompProperties_Biocodable"></li>
    </comps>
  </thingDef>
</Defs>
