<Patch>
	<!-- Aelanna's Model No-Match Detector and Insertion Code -->
	<!-- Special thanks to Aelanna for straight-up saving me scores of headaches! -->
	<!-- Add comps field to Settlement WorldObjectDef if it doesn't exist -->
	<Operation Class="PatchOperationConditional">
		<xpath>/Defs/ThingDef[@Name="DankPyon_Base_Pants"]/apparel</xpath>
		<nomatch Class="PatchOperationAdd">
			<xpath>/Defs/ThingDef[@Name="DankPyon_Base_Pants"]</xpath>
			<success>Always</success>
			<value>
				<apparel></apparel>
			</value>
		</nomatch>
	</Operation>
	
	<!-- Add comps field to Settlement WorldObjectDef if it doesn't exist -->
	<Operation Class="PatchOperationConditional">
		<xpath>/Defs/ThingDef[defName="DankPyon_Apparel_Hose"]/apparel</xpath>
		<nomatch Class="PatchOperationAdd">
			<xpath>/Defs/ThingDef[defName="DankPyon_Apparel_Hose"]</xpath>
			<success>Always</success>
			<value>
				<apparel></apparel>
			</value>
		</nomatch>
	</Operation>
	
	<Operation Class="PatchOperationConditional">
	<!-- Does this exist? -->
		<xpath>/Defs/ThingDef[defName="DankPyon_Apparel_Hose"]/apparel/developmentalStageFilter</xpath>
		<!-- If it does, do this replacement. -->
		<match Class="PatchOperationReplace">
			<xpath>/Defs/ThingDef[defName="DankPyon_Apparel_Hose"]/apparel/developmentalStageFilter</xpath>
			<success>Always</success>
			<value>
				<developmentalStageFilter>Baby, Child, Adult</developmentalStageFilter>
			</value>
		</match>
		<!-- If it DOES NOT, add this instead. -->
		<nomatch Class="PatchOperationAdd">
			<xpath>/Defs/ThingDef[defName="DankPyon_Apparel_Hose"]/apparel</xpath>
